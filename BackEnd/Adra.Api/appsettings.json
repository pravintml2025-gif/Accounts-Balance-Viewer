{
  "Jwt": {
    "Issuer": "https://adra-api",
    "Audience": "https://adra-web",
    "TokenLifetimeInMinutes": 120
  },
  "FileUpload": {
    "MaxFileSizeInBytes": 10485760,
    "AllowedExtensions": [".csv", ".tsv", ".txt", ".xlsx", ".xls"],
    "UploadPath": "uploads",
    "MaxRecordsPerFile": 10000
  },
  "RateLimit": {
    "PermitLimit": 100,
    "WindowInMinutes": 1,
    "QueueLimit": 10,
    "UploadPermitLimit": 10,
    "UploadWindowInMinutes": 5
  },
  "Cors": {
    "AllowedOrigins": [],
    "AllowCredentials": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Adra": "Information"
    }
  },
  "AllowedHosts": "*"
}
